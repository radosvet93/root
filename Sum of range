function range(start, end, step) {
    if (step == null) step = 1;
    var arr = [];

    if (step > 0) {
        for (let i = start; i <= end; i += step)
            arr.push(i);
    } else {
        for (let i = start; i >= end; i += step)
            arr.push(i);
    }
    return arr;
}
function sum(arr) {
    var result = 0;
    for (let i = 0; i < arr.length; i += 1)
        result += arr[i];
    return result;

}
