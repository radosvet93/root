using System;


namespace PrintSeq
{
    class PrintSeq
    {
        static void Main()
        {
            for (int i = 2; i < 200; i += 1)
                if (i % 2 == 0)
                {
                    Console.WriteLine("{0}", i);
                }
                else
                {
                    Console.WriteLine("{0}", -i);
                }
            Console.WriteLine();
        }
    }
}
