/*jshint esversion: 6 */
/* jshint -W097 */
/* jshint -W117 */
'use strict';


var size = 8;
var board = "";
for(var x = 0; x < size; x += 1) {
    for(var y = 0; y < size; y += 1) {
        if ((x + y)% 2 === 0) 
            board += " ";
        else
            board += "#";
        
    }
    board += "\n";
}

console.log(board);
